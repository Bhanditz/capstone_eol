<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    This defines the extension for defining EOL data objects as extensions to a Taxon core object.
    This will be used by EOL content partners who will use the IPT to export their data to a format
    that can be ingested into EOL.
-->
<extension xmlns="http://ipt.gbif.org/extension/"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://ipt.gbif.org/extension/ http://gbrds.gbif.org/resources/schemas/extension.xsd"
            title="EOL ContentSchema 2.0"
            name="EOL ContentSchema 2.0"
            namespace="http://www.eol.org/transfer/content/2.0/">

    <property name="identifier"
              namespace="http://purl.org/dc/terms/"
              qualName="http://purl.org/dc/terms/identifier"/>

    <property name="name"
              namespace="http://xmlns.com/foaf/0.1/"
              qualName="http://xmlns.com/foaf/0.1/name"/>

    <property name="firstName"
              namespace="http://xmlns.com/foaf/0.1/"
              qualName="http://xmlns.com/foaf/0.1/firstName"/>

    <property name="familyName"
              namespace="http://xmlns.com/foaf/0.1/"
              qualName="http://xmlns.com/foaf/0.1/familyName"/>

    <property name="email"
              namespace="http://xmlns.com/foaf/0.1/"
              qualName="http://xmlns.com/foaf/0.1/mbox"/>

    <property name="homepage"
              namespace="http://xmlns.com/foaf/0.1/"
              qualName="http://xmlns.com/foaf/0.1/homepage"/>

    <property name="logoURL"
              namespace="http://xmlns.com/foaf/0.1/"
              qualName="http://xmlns.com/foaf/0.1/img"/>

    <property name="accountName"
              namespace="http://xmlns.com/foaf/0.1/"
              qualName="http://xmlns.com/foaf/0.1/accountName"/>

    <!-- for potentially connecting with Flickr -->
    <property name="yahooChatID"
              namespace="http://xmlns.com/foaf/0.1/"
              qualName="http://xmlns.com/foaf/0.1/yahooChatID"/>
</extension>