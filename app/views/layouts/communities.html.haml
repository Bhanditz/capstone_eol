- content_for :head do
  = javascript_include_tag 'communities'
  = stylesheet_link_tag 'communities'
- content_for :content do
  #content
    #page-separator-general
    #page-title
      %h1
        = @page_title || 'EOL Communities'
      %hr
    #page-content.clearfix
      #full-page-content
        = yield
= render :file => 'layouts/main'
