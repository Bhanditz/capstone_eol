- linked ||= false
- supplier ||= false
= render :partial => 'data_objects/license', :locals => { :data_object => data_object }
.copy
  %p<
    = render :partial => 'data_objects/owner', :locals => { :data_object => data_object, :linked => true }
    - if linked
      &mdash;
      = link_to I18n.t(:source_information_link), data_object_path(data_object)
  - if supplier
    %p
      = I18n.t(:supplier_colon) + " "
      = link_to data_object.data_supplier_agent.full_name, data_object.data_supplier_agent.user