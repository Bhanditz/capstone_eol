=I18n.t("this_information_will_allow_us")
= eol_lang_error_messages_for :content_partner_contact, :header_message => I18n.t(:contact_errors) 
- form_for @content_partner_contact, :url => { :action => 'add_contact' }, :html => { :method => :post, :id => 'contact_form', :class => "warning-warn invalid-invalid styleLabelOnErr" } do |f|
  = render :partial => 'contact_form', :locals => { :f => f }
  .button
    = submit_tag I18n.t(:update_account) 
